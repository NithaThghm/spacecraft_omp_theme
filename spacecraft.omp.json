{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palettes": {
    "list": {
      "Sublette": {
        "spc-black": "#202535",
        "spc-lightblack": "#202535",
        "spc-brightblack": "#405570",
        "spc-blue": "#58f",
        "spc-darkerblue": "#0E62C2",
        "spc-lightblue": "#7bf",
        "spc-cyan": "#4ee",
        "spc-brightcyan": "#5fb",
        "spc-green" :"#5e7",
        "spc-term-green": "#93ff91",
        "spc-term-green2": "#49ff6d",
        "spc-term-green3": "#16a085",
        "spc-term-green4": "#205c57",
        "spc-orange" : "#e57c46",
        "spc-red": "#e57",
        "spc-brightred" : "#e65",
        "spc-silver" : "#cccccc"
      }
    },
    "template": "Sublette"
  },
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "p:spc-silver",
          "properties": {
            "windows": "\ue70f"
          },
          "style": "plain",
          "template": " {{ .Icon }} ",
          "type": "os"
        },
        {
          "foreground": "lightYellow",
          "style": "plain",
          "template": "{{ .UserName }} ",
          "type": "session"
        },
        {
          "type": "path",
          "template": "<p:spc-silver><p:spc-blue> \uf07c </>:</> {{ .Path }} ",
          "style": "plain",
          "properties": {
            "style": "folder"
          },
          "foreground": "p:spc-term-green2"
        },
        {
          "type": "git",
          "templates": [
            "<p:spc-silver,p:spc-orange><b> \uf1d3 :</b></>",
            "{{.HEAD}}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }}"
          ],
          "style": "plain",
          "properties": {
            "branch_icon": " <b>\ue725</b> ",
            "fetch_bare_info": true,
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "fetch_stash_count": true
          },
          "foreground": "p:spc-term-green",
          "background": "p:spc-lightblack"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "lightGreen",
          "style": "plain",
          "template": "\u276f",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 2
}
