{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "final_space": true,
  "version": 2,
  "palettes": {
    "template": "Sublette",
    "list": {
      "Sublette": {
        "spc-background": "#202535",
        "spc-badge-background": "transparent",
        "spc-badge-background2": "#414962",
        "spc-black": "#202535",
        "spc-lightblack": "#202535",
        "spc-brightblack": "#405570",
        "spc-blue": "#58f",
        "spc-darkerblue": "#0E62C2",
        "spc-lightblue": "#7bf",
        "spc-cyan": "#4ee",
        "spc-brightcyan": "#5fb",
        "spc-green" :"#5e7",
        "spc-term-green": "#93ff91",
        "spc-term-green2": "#49ff6d",
        "spc-term-green3": "#16a085",
        "spc-term-green4": "#205c57",
        "spc-magenta": "#f7c",
        "spc-magenta2": "#FF78B4",
        "spc-orange" : "#e57c46",
        "spc-orange2": "#F78E69",
        "spc-red": "#e57",
        "spc-red2": "#CA1551",
        "spc-brightred" : "#e65",
        "spc-silver" : "#d9d9d9",
        "spc-yellow" : "#fd8",
        "spc-brightyellow" : "#ff7"
      }
    }
  },
  "var": {
    "leading_badge": "<p:spc-badge-background,p:spc-background>\ue0b0 </>",
    "trailing_badge": "<p:spc-badge-background,p:spc-background>\ue0b0 </>",
    "badge_separator": "<p:spc-yellow>\u00bb</>",
    "badge_separator2": "<p:spc-yellow>\uf0da</>"
  },
  "blocks": [
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "os",
          "templates": [
            "<b> {{ .Icon }}</b> {{ .Var.badge_separator }} ",
            "<b> {{ .Icon }}</b> {{ .Var.trailing_badge }}"
          ],
          "templates_logic": "first_match",
          "style": "plain",
          "properties": {
            "windows": "\ue70f"
          },
          "foreground": "p:spc-silver",
          "background": "p:spc-badge-background"
        },
        {
          "type": "",
          "templates": [
            "{{ .UserName }} "
          ],
          "templates_logic": "first_match",
          "style": "plain",
          "foreground": "p:spc-yellow",
          "background": "p:spc-background"
        },
        {
          "type": "path",
          "templates": [
            "{{ if .Segments.Git.HEAD }}<p:spc-green>\uf1d3 </>{{ else }}<p:spc-red>\uf1d3 </>{{ end }}",
            "<b>\uf07c </b>{{ .Var.badge_separator }} "
          ],
          "style": "plain",
          "foreground": "p:spc-silver",
          "background": "p:spc-badge-background"
        },
        {
          "type": "path",
          "templates": [
            "{{ .Path }} "
          ],
          "style": "plain",
          "properties": {
            "style": "agnoster_short",
            "folder_separator_icon": "/",
            "hide_root_location": true
          },
          "foreground": "p:spc-lightblue",
          "background": "p:spc-badge-bac"
        },
        {
          "type": "git",
          "templates": [
            "{{ .Var.badge_separator }} ",
            "{{ .BranchStatus }} ",
            "{{ .UpstreamIcon }}  "
          ],
          "style": "plain",
          "properties": {
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "branch_identical_icon": "<p:spc-green>\u2261</>",
            "branch_ahead_icon": "<p:spc-red>\u2191</>",
            "branch_behind_icon": "<p:spc-red>\u2193</>",
            "branch_gone_icon": "<p:spc-red>\uf4ad </>"
          },
          "foreground": "p:spc-silver",
          "background": "p:spc-badge-background"
        },
        {
          "type": "git",
          "templates": [
            "{{.HEAD}} ",
            "",
            "[",
            "{{ .Working }}",
            "{{ if gt .StashCount 0 }} · \ue257 {{ .StashCount }}{{end}}",
            "] "
          ],
          "style": "plain",
          "properties": {
            "branch_icon": "<b>\ue725 </b>",
            "fetch_bare_info": true,
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "fetch_stash_count": true,
            "github_icon": "\ueb00"
          },
          "foreground": "p:spc-magenta2"
        }
      ],
      "type": "prompt"
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "type": "time",
          "properties": {
            "time_format": "15:04"
          }
        },
        {
          "style": "plain",
          "type": "executiontime",
          "template": "<p:spc-yellow>\uf0e7</> {{ .FormattedMs }}",
          "properties": {
            "style": "austin",
            "always_enabled": true
          }
        },
        {
          "type": "shell",
          "properties": {
            "mapped_shell_names": {
              "bash" : "· \uebca"
            }
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "template": "\ueab6",
          "foreground": "p:spc-green"
        }
      ]
    }
  ]
}
