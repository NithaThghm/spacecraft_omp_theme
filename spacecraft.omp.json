{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "final_space": true,
  "version": 2,
  "palettes": {
    "template": "Sublette",
    "list": {
      "Sublette": {
        "spc-background": "#202535",
        "spc-badge-background": "#414962",
        "spc-black": "#202535",
        "spc-lightblack": "#202535",
        "spc-brightblack": "#405570",
        "spc-blue": "#58f",
        "spc-darkerblue": "#0E62C2",
        "spc-lightblue": "#7bf",
        "spc-cyan": "#4ee",
        "spc-brightcyan": "#5fb",
        "spc-green" :"#5e7",
        "spc-term-green": "#93ff91",
        "spc-term-green2": "#49ff6d",
        "spc-term-green3": "#16a085",
        "spc-term-green4": "#205c57",
        "spc-magenta": "#f7c",
        "spc-orange" : "#e57c46",
        "spc-red": "#e57",
        "spc-brightred" : "#e65",
        "spc-silver" : "#d9d9d9",
        "spc-yellow" : "#fd8",
        "spc-brightyellow" : "#ff7"
      }
    }
  },
  "blocks": [
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "os",
          "template": "<p:spc-silver,p:spc-badge-background><b> {{ .Icon }} </b></><p:spc-badge-background,p:spc-background>\ue0b0 </>",
          "style": "plain",
          "properties": {
            "windows": "\ue70f"
          }
        },
        {
          "type": "session",
          "template": "{{ .UserName }} ",
          "style": "plain",
          "foreground": "p:spc-yellow"
        },
        {
          "type": "path",
          "template": "<p:spc-silver,p:spc-badge-background><b> \uf07c </b></><p:spc-badge-background,p:spc-background>\ue0b0 </>{{ .Path }} ",
          "style": "plain",
          "properties": {
            "style": "folder"
          },
          "foreground": "p:spc-lightblue"
        },
        {
          "type": "git",
          "templates": [
            "<p:spc-silver,p:spc-badge-background><b> \uf1d3 </b></><p:spc-badge-background,p:spc-background>\ue0b0</></>",
            "",
            "{{.HEAD}}",
            "{{ .UpstreamIcon }}",
            " [",
            "{{ if gt .StashCount 0 }}\ue257 {{ .StashCount }}{{end}}",
            "]"
          ],
          "style": "plain",
          "properties": {
            "branch_icon": " <p:spc-magenta><b>\ue725 </b></>",
            "fetch_bare_info": true,
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "fetch_stash_count": true
          },
          "foreground": "p:spc-magenta"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "lightGreen",
          "style": "plain",
          "template": "\u276f",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ]
}
